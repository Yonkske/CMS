<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.dhbw.StartpageController">
   <children>
      <HBox alignment="TOP_RIGHT" prefHeight="23.0" prefWidth="600.0">
         <children>
            <Label fx:id="adminLbl" text="Vorname, Nachname">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></Label>
         </children>
      </HBox>
      <HBox prefHeight="389.0" prefWidth="600.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="startpageBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToStartpage" text="Startseite">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="citBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToCitAdmin" text="CIT">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="statisticsBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToStatisticAdmin" text="Statistik">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="userBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToUserAdmin" text="Benutzer">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin></Button>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <VBox prefHeight="383.0" prefWidth="274.0">
               <children>
                  <HBox prefHeight="33.0" prefWidth="274.0">
                     <children>
                        <TextField fx:id="searchTf">
                           <HBox.margin>
                              <Insets top="20.0" />
                           </HBox.margin></TextField>
                        <Button fx:id="searchBtn" mnemonicParsing="false" onAction="#searchCirOrCit" text="Suchen">
                           <HBox.margin>
                              <Insets left="15.0" top="20.0" />
                           </HBox.margin></Button>
                     </children>
                  </HBox>
                  <TableView fx:id="cirTable" prefHeight="276.0" prefWidth="270.0">
                    <columns>
                      <TableColumn fx:id="citColumn" editable="false" prefWidth="135.0" text="CIT">
                           <graphic>
                              <ComboBox fx:id="filterCitCb" prefHeight="25.0" prefWidth="135.0" promptText="CIT" onAction="#setFilterForTable" />
                           </graphic>
                        </TableColumn>
                      <TableColumn fx:id="cirNameColumn" editable="false" prefWidth="135.0" text="CIR-Bezeichnung" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox prefHeight="383.0" prefWidth="228.0">
               <children>
                  <HBox alignment="TOP_RIGHT" prefHeight="27.0" prefWidth="228.0">
                     <children>
                        <Button fx:id="citEditBtn" mnemonicParsing="false" onAction="#openAddCit" text=" CIT erstellen">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="citDeleteBtn" mnemonicParsing="false" onAction="#openDeleteCitPopup" text="CIT löschen">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets top="272.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" prefHeight="32.0" prefWidth="228.0">
                     <children>
                        <Button fx:id="cirAddBtn" mnemonicParsing="false" onAction="#openAddCir" text="CIR erstellen">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="cirEditBtn" mnemonicParsing="false" onAction="#openEditCir" text="CIR bearbeiten">
                           <HBox.margin>
                              <Insets right="5.0" />
                           </HBox.margin>
                        </Button>
                        <Button fx:id="cirDeleteBtn" mnemonicParsing="false" onAction="#openDeleteCirPopup" text="CIR löschen">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <VBox.margin>
                        <Insets top="6.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
