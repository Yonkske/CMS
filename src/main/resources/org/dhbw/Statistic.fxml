<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="TOP_RIGHT" minHeight="575.0" minWidth="575.0" style="-fx-background-color: #f0f0f8;"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.dhbw.StatisticController">
   <children>
      <HBox alignment="TOP_RIGHT" minHeight="37.0" style="-fx-background-color: #313c48;">
         <children>
            <MenuButton fx:id="adminUserMB" mnemonicParsing="false" style="-fx-background-color: #313c48;"
                        text="Vorname, Nachname" textFill="WHITE">
               <items>
                  <MenuItem mnemonicParsing="false">
                     <graphic>
                        <Button fx:id="benutzerDropDownBtn" mnemonicParsing="false" onAction="#openUserInfo"
                                text="Benutzer"/>
                     </graphic>
                  </MenuItem>
                  <MenuItem fx:id="itemPassword" mnemonicParsing="false">
                     <graphic>
                        <Button fx:id="passwordEditBtn" mnemonicParsing="false" onAction="#openChangePassword" text="Passwort Ã¤ndern" />
                     </graphic>
                  </MenuItem>
               </items>
               <font>
                  <Font name="Hiragino Sans W2" size="15.0" />
               </font>
            </MenuButton>
            <Button fx:id="logoutBtn" mnemonicParsing="false" onAction="#logOut" prefHeight="36.0" prefWidth="78.0"
                    style="-fx-background-color: #313c48;" text="Logout" textFill="WHITE">
               <font>
                  <Font name="Hiragino Sans W2" size="15.0"/>
               </font>
            </Button>
         </children>
      </HBox>
      <HBox minWidth="85.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="TOP_CENTER" maxWidth="170.0" minWidth="85.0" style="-fx-background-color: #3D4956;"
                  HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="startpageBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToStartpage"
                          style="-fx-background-color: #3D4956;" text="Startseite" textFill="WHITE">
                     <VBox.margin>
                        <Insets top="20.0"/>
                     </VBox.margin>
                     <font>
                        <Font name="Hiragino Sans W2" size="15.0"/>
                     </font>
                  </Button>
                  <Button fx:id="citBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToCit"
                          style="-fx-background-color: #3D4956;" text="CIT" textFill="WHITE">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Hiragino Sans W2" size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="statisticBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToStatistic" style="-fx-background-color: #3D4956;" text="Statistik" textFill="WHITE">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Hiragino Sans W2" size="15.0" />
                     </font>
                  </Button>
                  <Button fx:id="userBtn" minWidth="65.6" mnemonicParsing="false" onAction="#swapToUserAdmin" style="-fx-background-color: #3D4956;" text="Benutzer" textFill="WHITE">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Hiragino Sans W2" size="15.0" />
                     </font>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <VBox minWidth="490.0">
               <children>
                  <HBox prefHeight="35.0" prefWidth="323.0">
                     <VBox.margin>
                        <Insets top="20.0"/>
                     </VBox.margin>
                  </HBox>
                  <VBox prefHeight="74.0" prefWidth="323.0">
                     <children>
                        <HBox prefHeight="236.0" prefWidth="470.0">
                           <children>
                              <Label fx:id="cirLabel" minWidth="30.0" text="Anzahl CIR">
                                 <HBox.margin>
                                    <Insets left="20.0" right="40.0" />
                                 </HBox.margin>
                                 <font>
                                    <Font name="Hiragino Sans W2" size="15.0" />
                                 </font></Label>
                              <TextField fx:id="numberCIRTf" alignment="TOP_CENTER" disable="true" prefWidth="50.0" style="-fx-background-color: #c4c4cc;">
                                 <HBox.margin>
                                    <Insets bottom="15.0" />
                                 </HBox.margin></TextField>
                           </children>
                        </HBox>
                        <HBox prefHeight="27.0" prefWidth="251.0">
                           <children>
                              <Label fx:id="citLabel" minWidth="30.0" text="Anzahl CIT">
                                 <font>
                                    <Font name="Hiragino Sans W2" size="15.0" />
                                 </font>
                                 <HBox.margin>
                                    <Insets left="20.0" right="40.0" />
                                 </HBox.margin></Label>
                              <TextField fx:id="numberCITTf" alignment="TOP_CENTER" disable="true" editable="false" prefWidth="50.0" style="-fx-background-color: #c4c4cc;" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <HBox prefHeight="253.0" prefWidth="393.0">
                     <children>
                        <PieChart fx:id="adminPieChart" HBox.hgrow="ALWAYS">
                           <effect>
                              <Blend/>
                           </effect>
                        </PieChart>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
